pipleline {
  agent any
  stages {
    stage("build") {
      steps {
        echo 'retoring packages'
        echo 'build app'
      }
    }
    
    stage("test") {
      steps {
          echo 'testing packages'
      }
    }
    
    stage("deploy") {
      steps {
        echo 'deploying packages'
      }
    }
  }
}
